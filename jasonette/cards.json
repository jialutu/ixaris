{
  "$jason": {
    "head": {
      "data": {
        "corporations": {
            "type": "$network.request",
            "options": {
              "url": "http://ec3a1990.ngrok.io/cards",
              "method": "get"
            }
        },
        "templates": {
          "body": {
            "sections": {
              "{{#each corporations}}": [
                {
                  "type": "horizontal",
                  "items": {
                    "{{#each items}}": {
                      "type": "vertical",
                      "style": {
                        "background": "#fafafa",
                        "padding": "9",
                        "spacing": "13",
                        "width": "90",
                        "align": "center",
                        "distribution": "equalspacing",
                        "height": "120"
                      },
                      "components": [
                        {
                          "type": "label",
                          "url": "{{currency}}",
                          "style": {
                            "size": "12"
                          }
                        },
                        {
                          "type": "label",
                          "text": "{{cardNumber}}",
                          "style": {
                            "size": "12"
                          }
                        }
                      ]
                    }
                  }
                }
              ]
            }
          }
        }
      }
    }
  }
}
