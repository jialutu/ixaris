{
  "$jason": {
    "head": {
      "data": {
        "corporations": [
          {
            "currency": "GBP",
            "cardNumber": "1"
          }
        ]
      },
        "templates": {
          "body": {
            "header": {
            "title": "All cards",
            "style": {
              "background": "#646464",
              "color": "#ffffff"
            }
          },
            "sections": {
              "{{#each corporations}}": [
                {
                  "type": "horizontal",
                  "items": {
                    "type": "vertical",
                    "style": {
                      "background": "#fafafa",
                      "padding": "9",
                      "spacing": "13",
                      "width": "90",
                      "align": "center",
                      "distribution": "equalspacing",
                      "height": "120"
                    },
                    "components": [
                      {
                        "type": "label",
                        "url": "{{currency}}",
                        "style": {
                          "size": "12"
                        }
                      },
                      {
                        "type": "label",
                        "text": "{{cardNumber}}",
                        "style": {
                          "size": "12"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
    }
  }
}
